<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx">
  <edmx:DataServices m:DataServiceVersion="2.0" xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
    <Schema Namespace="mock" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
      
      <EntityType Name="Employees">
        <Key>
          <PropertyRef Name="EmployeeID"/>
        </Key>
        <Property Name="EmployeeID" Type="Edm.String" MaxLength="40"/>
        <Property Name="FirstName" Type="Edm.String" MaxLength="40"/>
        <Property Name="LastName" Type="Edm.String" MaxLength="40"/>
        <Property Name="Age" Type="Edm.Int16" MaxLength="2"/>
        <Property Name="DateHire" Type="Edm.DateTime"/>
        <Property Name="CareerLevel" Type="Edm.String" MaxLength="2"/>
        <Property Name="CurrentProject" Type="Edm.String" MaxLength="40"/>
      </EntityType>

      <EntityType Name="ProjectList">
        <Key>
          <PropertyRef Name="ProjectId"/>
        </Key>
        <Property Name="ProjectId" Type="Edm.String" MaxLength="10"/>
        <Property Name="ProjectName" Type="Edm.String" MaxLength="40"/>
      </EntityType>

      <EntityType Name="Skills">
        <Key>
          <PropertyRef Name="EmployeeID"/>
        </Key>
        <Property Name="EmployeeID" Type="Edm.String" MaxLength="40"/>
        <Property Name="SkillId" Type="Edm.String" MaxLength="10"/>
        <Property Name="SkillName" Type="Edm.String" MaxLength="20"/>
        <Property Name="ProficiencyID" Type="Edm.String" MaxLength="2"/>
        <Property Name="ProficiencyLevel" Type="Edm.String" MaxLength="2"/>
      </EntityType>

      <EntityType Name="ProficiencyList">
        <Key>
          <PropertyRef Name="ProficiencyId"/>
        </Key>
        <Property Name="ProficiencyId" Type="Edm.String" MaxLength="10"/>
        <Property Name="ProficiencyLevel" Type="Edm.String" MaxLength="2"/>
      </EntityType>

      <EntityType Name="SkillList">
        <Key>
          <PropertyRef Name="SkillId"/>
        </Key>
        <Property Name="SkillId" Type="Edm.String" MaxLength="10"/>
        <Property Name="SkillName" Type="Edm.String" MaxLength="20"/>
      </EntityType>

      <EntityType Name="CareerList">
        <Key>
          <PropertyRef Name="CareerId"/>
        </Key>
        <Property Name="CareerId" Type="Edm.String" MaxLength="10"/>
        <Property Name="CareerLevel" Type="Edm.String" MaxLength="2"/>
      </EntityType>

      <EntityContainer Name="EmployeeService" m:IsDefaultEntityContainer="true">
        <EntitySet Name="Employees" EntityType="mock.Employees"/>
        <EntitySet Name="ProjectList" EntityType="mock.ProjectList"/>
        <EntitySet Name="Skills" EntityType="mock.Skills"/>
        <EntitySet Name="ProficiencyList" EntityType="mock.ProficiencyList"/>
        <EntitySet Name="SkillList" EntityType="mock.SkillList"/>
        <EntitySet Name="CareerList" EntityType="mock.CareerList"/>
      </EntityContainer>
      
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>